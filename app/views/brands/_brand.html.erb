<article id="<%= dom_id brand %>" class="py-6 prose dark:prose-invert">
    <p class="mb-0 font-semibold">
      Name
    </p>
    <p class="my-0">
      <%= brand.name %>
    </p>
    <p class="mb-0 font-semibold">
      Description
    </p>
    <p class="my-0">
      <%= brand.description %>
    </p>
  <time class="text-slate-600 dark:text-slate-400 text-xs mt-2" datetime="<%= brand.created_at.to_formatted_s(:long) %>">Created <%= time_ago_in_words(brand.created_at) + " ago" %></time>
</article>
