<article id="<%= dom_id stack %>" class="py-6 prose dark:prose-invert">
  <p class="mb-0 font-semibold">
    Title
  </p>
  <p class="my-0">
    <%= stack.title %>
  </p>

  <time class="text-slate-600 dark:text-slate-400 text-xs mt-2" datetime="<%= stack.created_at.to_formatted_s(:long) %>">Created <%= time_ago_in_words(stack.created_at) + " ago" %></time>
</article>
